{
  "default": {
    "extensions": [".py", ".md"],
    "files": ["Dockerfile", "docker-compose.yml", "dldc", "run"]
  },
  "docs": {
    "extensions": [".md"],
    "first_file": "README.md"
  },
  "diff": {
    "command": "git diff --name-only"
  },
  "git": {
    "files": [".git/config", ".gitignore"]
  }
}
